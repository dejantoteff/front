{
	"snapshotSerializers": [
		"enzyme-to-json/serializer"
	],
	"setupFiles": [
		"<rootDir>/files/jest/testShim.js",
		"<rootDir>/files/jest/testSetup.js"
	],
	"setupTestFrameworkScriptFile": "<rootDir>/files/jest/testScript.js",
	"moduleFileExtensions": [
		"ts",
		"tsx",
		"js"
	],
	"transform": {
		"^.+\\.(ts|tsx)$": "<rootDir>/files/jest/testPreprocessor.js"
	}
}